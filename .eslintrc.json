{
  "env": {
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/prefer-default-export": ["off"],
    "no-await-in-loop": ["off"],
    "no-constant-condition": ["off"],
    "import/extensions": ["off"]
  }
}
